{
  "name": "obsidian-sample-plugin",
  "version": "1.0.0",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "dist/main.js",
  "scripts": {
    "dev": "node ./scripts/esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node ./scripts/esbuild.config.mjs production",
    "css": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "version": "node ./scripts/version-bump.mjs && git add manifest.json versions.json",
    "pretty": "prettier --write 'src/**/*.{js,ts,jsx,tsx}'"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.14.47",
    "obsidian": "latest",
    "tailwindcss": "^3.2.4",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
